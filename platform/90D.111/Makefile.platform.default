# 90D 1.1.1

#CANON_NAME_FIR  = CCF18101.FIR
FIRMWARE_ID     = 0x80000417
#UPDATE_NAME_FIR = LOG_850D.FIR
ROMBASEADDR     = 0xE0040000

#CONFIG_MMIO_TRACE = y

# user_mem_start from cstart struct. Align to 0x10 up
